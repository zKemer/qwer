                    <h3>
                      Onaylanan Botlar
                    </h3>
                    <table class="table">
                      <thead>
                        <tr>
                          <th scope="col">Resim</th>
                          <th scope="col">Bot Adı</th>
                          <th scope="col">Prefix</th>
                          <th scope="col">Oy Sayısı</th>
                          <th scope="col">Geliştirici</th>
                          <th scope="col">Sertifika</th>
                          <th scope="col">Seçenekler</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i = 0; i < Object.keys(system.db.fetch('bots')).length; i++) { %>
                        <% let id = Object.keys(system.db.fetch('bots'))[i] %>
                        <% if (system.db.fetch(`bots.${id}.verified`) == true) { %>
                        <% const bot = system.db.fetch(`bots.${id}`) %>
                        <tr>
                          <th scope="row"><img class="smallPP" src="https://cdn.discordapp.com/avatars/<%=bot.id%>/<%=bot.avatar%>"></th>
                          <td><%=bot.username%>#<%=bot.discriminator%></td>
                          <td><%=bot.prefix%></td>
                          <td><%=bot.votes%></td>
                          <td><%=bot.owner%>#<%=bot.ownerDiscriminator%></td>
                          <td><%=bot.certificate == "waiting" ? "Beklemede" : bot.certificate == true ? "Mevcut" : "Yok"%></td>
                          <td><a data-toggle="modal" data-target="#modal-<%=bot.id%>" class="btn btn-primary" style="color: white;">İşlem Yap &#160<i class="fad fa-angle-down"></i></a></td>
                        </tr>
                        
                        <div class="modal fade" id="modal-<%=bot.id%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                          <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content" style="border-radius: 20px; background-color: #2471A3;">
                              <div class="modal-header" style="border-bottom: 2px solid #1A5276;">
                                <h5 class="modal-title" style="color: white;" id="exampleModalCenterTitle"><img class="smallPP" src="https://cdn.discordapp.com/avatars/<%=bot.id%>/<%=bot.avatar%>">&#160 <%=bot.username%><b style="opacity: .4;">#<%=bot.discriminator%></b> &#160<span class="badge" style="background-color: #17202A; border: 2px solid #17202A; color: white;">İşlemler</span></h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                  <style>#closeButton:select { outline: none; } #closeButton { transition: .2; }</style>
                                  <i id="closeButton" area-hidden="true" class="fad fa-times-circle" style="transition: .2; color: white;"></i>
                                </button>
                              </div>
                              <div class="modal-body" style="color: #fff;">
                                <a href="https://discord.com/api/oauth2/authorize?client_id=<%=bot.id%>&permissions=0&scope=bot" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #A569BD;" class="fad fa-envelope-open-text"></i>&#160 Davet Linki &#160<span class="badge" style="background-color: rgba(165,105,189,.5);">Botu sunucuya eklemek için davet linkidir.</span></a>
                                <hr style="border: 1px solid #1A5276;">
                                <a href="/bot/<%=bot.id%>" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #2ECC71;" class="fad fa-eye"></i>&#160 Görüntüle &#160<span class="badge" style="background-color: rgba(46,204,113,0.5);">Botun sayfasını görüntülemek için tıklayınız.</span></a>
                                <hr style="border: 1px solid #1A5276;">
                                <% if (system.admins.includes(user.id)) { %>
                                <% if (bot.certificate == true) { %>
                                <a href="/panel/bots/removeSer/<%=bot.id%>" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #641E16;" class="fad fa-file-certificate"></i>&#160 Sertifika Kaldır &#160<span class="badge" style="background-color: rgba(100,30,22,.7);">Botun sertifikasını siler.</span></a>
                                <hr style="border: 1px solid #1A5276;">
                                <% } %>
                                <a href="#" data-toggle="modal" data-target="#remove-<%=bot.id%>" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #EC7063;" class="fad fa-trash-alt"></i>&#160 Botu Kaldır &#160<span class="badge" style="background-color: rgba(236,112,99,.7);">Botu siteden kaldırır.</span></a>
                                <% } else { %>
                                <% if (bot.certificate == true) { %>
                                <a disabled data-toggle="tooltip" data-placement="top" title="Gerekli Yetki: Admin" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #641E16;" class="fad fa-file-certificate"></i>&#160 Sertifika Kaldır &#160<span class="badge" style="background-color: rgba(100,30,22,.7);">Botun sertifikasını siler.</span></a>
                                <hr style="border: 1px solid #1A5276;">
                                <% } %>
                                <a disabled data-toggle="tooltip" data-placement="top" title="Gerekli Yetki: Admin" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #EC7063;" class="fad fa-trash-alt"></i>&#160 Botu Kaldır &#160<span class="badge" style="background-color: rgba(236,112,99,.7);">Botu siteden kaldırır.</span></a>
                                <% } %>
                              </div>
                            </div>
                          </div>
                        </div>
                        
                        <div class="modal fade" id="remove-<%=bot.id%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                          <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content" style="border-radius: 20px; background-color: #2471A3;">
                              <div class="modal-header" style="border-bottom: 2px solid #1A5276;">
                                <h5 class="modal-title" style="color: white;" id="exampleModalCenterTitle"><img class="smallPP" src="https://cdn.discordapp.com/avatars/<%=bot.id%>/<%=bot.avatar%>">&#160 <%=bot.username%><b style="opacity: .4;">#<%=bot.discriminator%></b> &#160<span class="badge" style="background-color: #922B21; border: 2px solid #922B21; color: white;">Botu Kaldır</span></h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                  <style>#closeButton:select { outline: none; } #closeButton { transition: .2; }</style>
                                  <i id="closeButton" area-hidden="true" class="fad fa-times-circle" style="transition: .2; color: white;"></i>
                                </button>
                              </div>
                              <div class="modal-body" style="color: #fff;">
                                <% if (system.admins.includes(user.id)) { %>
                                <form>
                                  <label for="blockedBotId"><i class="fad fa-robot"></i>&#160 Bot:</label>
                                  <input class="form-control" type="text" name="blockedBotId-<%=bot.id%>" id="blockedBotId-<%=bot.id%>" value="<%=bot.username%>#<%=bot.discriminator%>" disabled>
                                  <hr style="border: 1px solid #1A5276;">
                                  <label for="removeReason"><i class="fad fa-flag-checkered"></i>&#160 Sebep:</label>
                                  <input class="form-control" max-length="100" min-length="1" type="text" name="removeReason-<%=bot.id%>" id="removeReason-<%=bot.id%>">
                                  <hr style="border: 1px solid #1A5276;">
                                  <a id="removeButton-<%=bot.id%>" class="btn btn-danger" style="color: white;"><i class="fad fa-trash-alt"></i>&#160 Botu Kaldır</a>
                                  <script>
                                  $(document).ready(function(){
                                    $('#removeButton-<%=bot.id%>').click(function(){
                                      if (!$('#removeReason-<%=bot.id%>').val()) return alert("Hata: Lütfen bir sebep yazınız!");
                                      window.location.href = "/panel/bots/remove/<%=bot.id%>/" + $('#removeReason-<%=bot.id%>').val();
                                    });
                                  });
                                  </script>
                                </form>
                                <% } else { %>
                                <a disabled data-toggle="tooltip" data-placement="top" title="Gerekli Yetki: Admin" style="background: transparent; color: #fff; text-decoration: none;"><i style="color: #EC7063;" class="fad fa-crown"></i>&#160 Yetersiz Yetki &#160<span class="badge" style="background-color: rgba(236,112,99,.7);">Sayfayı görüntülemek içni iznin yok.</span></a>
                                <% } %>
                              </div>
                            </div>
                          </div>
                        </div>
                        
                        <% } %>
                        <% } %>
                      </tbody>
                    </table>


                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; 2020 Swôth - SpiX</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

