                        $(document).ready(function(){
                          $('#adminBanner').css('background-image', 'url("https://img.freepik.com/free-vector/blue-gradient-starry-night-background_23-2148261848.jpg?size=626&ext=jpg")');
                          $('#adminWelcome').text("İyi akşamlar, <%=user.username%>!")
                        });
                      </script>
                    <% } else { %>
                      <script>
                        $(document).ready(function(){
                          $('#adminBanner').css('background-image', 'url("https://images4.alphacoders.com/114/114688.jpg")');
                          $('#adminWelcome').text("İyi günler, <%=user.username%>!")
                        });
                      </script>
                    <% } %>

